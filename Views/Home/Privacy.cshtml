@{
    ViewData["Title"] = "Internship Landpage";
}
@section scripts {
    <script type="text/javascript" src="~/js/addNewcommers.js"></script>
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Internship Landpage</title>
</head>
<body>
    <div>
        <h1>@ViewData["Title"]</h1>
        <p>This is our classroom.</p>
        <ul id="list">
            @for (var i = 0; i < Model.Members.Count; i++)
            {
                var member = Model.Members[i];

                <li class="member">
                    @member
                </li>
            }
        </ul>
    </div>

    <input id="newcomer" title="Name" type="text" />
    <button id="add">Add new commer</button>
    <p><img src="/Images/GoodCodeRules.png" height="500" width="500" /></p>
    <script src="/js/addNewcommers.js"></script>
</body>
</html>
